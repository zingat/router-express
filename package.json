{
  "name": "router-express",
  "version": "1.4.7",
  "description": "Yet another routing for Express",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard *.js",
    "cover": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha test/*_spec.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha test/*_spec.js -- -R spec --bail"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yasinaydin/router-express"
  },
  "keywords": [
    "express",
    "routing",
    "router",
    "reverse",
    "app"
  ],
  "author": {
    "name": "Ozgur Yasin Aydin"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yasinaydin/router-express/issues"
  },
  "homepage": "https://github.com/yasinaydin/router-express",
  "dependencies": {
    "async": "^2.0.1",
    "lodash": "^4.15.0",
    "qs": "^6.2.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "coveralls": "^2.11.12",
    "express": "^4.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0-beta.5",
    "supertest": "^2.0.0"
  }
}
